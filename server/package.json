{
  "name": "blockreceipt-server",
  "version": "1.0.0",
  "description": "BlockReceipt.ai Express.js backend",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "tsx watch index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "dotenv-safe": "^8.2.0",
    "zod": "^3.22.4",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "ethers": "^6.10.0",
    "web3": "^4.3.0",
    "ws": "^8.14.2",
    "socket.io": "^4.7.4",
    "redis": "^4.6.11",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
